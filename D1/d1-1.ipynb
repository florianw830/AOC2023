    "f = open(\"input.txt\", \"r\")\n",
    "l = f.read().split(\"\\n\")[:-1]\n",
    "ll=[]\n",
    "for x in l:\n",
    "    tmp=\"\"\n",
    "    for z in x:\n",
    "        if ord(z)-48 <=9:\n",
    "            tmp = tmp + z\n",
    "    ll.append(int(tmp[0] + tmp[-1]))\n",
    "print(sum(ll))\n"
   ]
